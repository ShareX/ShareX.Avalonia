<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="using:ShareX.Editor.Views"
        xmlns:editorVm="using:ShareX.Editor.ViewModels"
        mc:Ignorable="d" d:DesignWidth="1200" d:DesignHeight="800"
        x:Class="ShareX.Ava.UI.Views.EditorWindow"
        x:DataType="editorVm:EditorViewModel"
        Title="ShareX Editor"
        Icon="/Assets/Logo.ico"
        WindowStartupLocation="CenterScreen">

    <views:EditorView Name="EditorViewControl">
        <views:EditorView.DataContext>
            <editorVm:EditorViewModel/>
        </views:EditorView.DataContext>
    </views:EditorView>
    
</Window>
